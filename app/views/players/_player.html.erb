<table>
<% if player.id == session[:player_id]%>
	<tr class="player" bg="red">
		<td>
			You!
		</td>
		<td>
			<%= player.state %>
		</td>
	</tr>
<% else %>	
	<tr class="player">
		<td>
			<%= player.name %>
		</td>
		<td>
			<%= player.state %>
		</td>	
	</tr>
<% end %>	
</table>
	
